{
  "kind": "collectionType",
  "collectionName": "product_cards",
  "info": {
    "singularName": "product-card",
    "pluralName": "product-cards",
    "displayName": "product-card",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "minLength": 5,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "minLength": 5
    },
    "price": {
      "type": "decimal",
      "required": true,
      "min": 2
    },
    "inStock": {
      "type": "boolean"
    },
    "img": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "supplierId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "brand": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}
